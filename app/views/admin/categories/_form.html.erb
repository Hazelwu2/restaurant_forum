<%= form_for [:admin, @restaurant] do |f| %>
    <div class="form-group">
        <%= f.label :category %>
        <%= f.collect_select :category_id, Category.all, :id, :name, {prompt: true}, {class: "form-control"} %>
    </div>
    <div class="form-group">
        <%= f.label :name, "Name" %>
        <%= f.text_field :name, class: "form-control" %>
    </div>
<% end %>